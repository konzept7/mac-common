    "prettier": "^2.8.4",
    "rollup-plugin-typescript2": "^0.34.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.2"
  },
  "dependencies": {
    "i18next": "^22.4.11"
